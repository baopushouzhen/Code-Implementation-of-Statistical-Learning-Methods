   "source": [
    "def pca(X):\n",
    "   \n",
    "    X = (X - X.mean()) / X.std()\n",
    "    \n",
    "    X = np.matrix(X)\n",
    "    cov = (X.T * X) / X.shape[0]\n",
    "   \n",
    "    U, S, V = np.linalg.svd(cov)\n",
    "    \n",
    "    return U, S, V"
   ]
